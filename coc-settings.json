{
  "diagnostic.enable": false,
  "diagnostic.displayByAle": true,
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"],
      "initializationOptions": {
        "usePlaceholders": true
      }
    }
  },
  "snippets.ultisnips.directories": [
    "UltiSnips"
  ],
  "snippets.userSnippetsDirectory": "~/.vim/snippets",
  "yaml.schemas": {
      "kubernetes": "/*.yaml"
  }
}
